<section class="mb-4 container contact">

  <!--Section heading-->
  <h2 class="h1-responsive font-weight-bold text-center my-4">Kontakt</h2>
  <!--Section description-->
  <p class="text-center w-responsive mx-auto mb-5">Masz jakieś pytania? Wypełnij formularz i skontaktuj się z nami.</p>

  <div class="row contact-form">

      <!--Grid column-->
      <div class=" col-sm-12 col-md-9 mb-md-0 mb-5">
          <form id="contact-form" name="contact-form" action="https://formspree.io/f/mqkvqjqo" method="POST">

              <!--Grid row-->
              <div class="row">

                  <!--Grid column-->
                  <div class=" col-sm-12 col-md-6">
                      <div class="md-form mb-0">
                        <label for="name" class="">Imię</label>
                          <input type="text" id="name" name="name" class="form-control">
                      </div>
                  </div>
                  <!--Grid column-->

                  <!--Grid column-->
                  <div class="col-md-6">
                      <div class="md-form mb-0">
                        <label for="email" class="">Adres email</label>
                          <input type="text" id="email" name="email" class="form-control">
                      </div>
                  </div>
                  <!--Grid column-->

              </div>
              <!--Grid row-->

              <!--Grid row-->
              <div class="row">
                  <div class="col-md-12 pt-5">
                      <div class="md-form mb-0">
                        <label for="subject" class="">Temat</label>
                          <input type="text" id="subject" name="subject" class="form-control">
                        </div>
                  </div>
              </div>
              <!--Grid row-->

              <!--Grid row-->
              <div class="row">

                  <!--Grid column-->
                  <div class="col-md-12 pt-5">

                      <div class="md-form ">
                        <label for="message">Twoja Wiadomość</label>
                          <textarea type="text" id="message" name="message" rows="2" class="form-control md-textarea"></textarea>
                      </div>

                  </div>
              </div>
              <!--Grid row-->

          </form>

          <div class="text-center text-md-left pt-3">
              <div class="form-btn-box pt-5">
                <a class=" form-btn mt-5 save-btn" onclick="document.getElementById('contact-form').submit();">Wyślij</a>
              </div>
          </div>
          <div class="status"></div>
      </div>
   

  </div>

  
</section>




